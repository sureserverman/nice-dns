<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" 
  "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>org.localdns.pf</string>

    <key>ProgramArguments</key>
    <array>
      <string>/usr/local/sbin/local-pf.sh</string>
    </array>

    <key>RunAtLoad</key>
    <true/>

    <key>StandardErrorPath</key>
    <string>/var/log/localdns-pf.err</string>
    <key>StandardOutPath</key>
    <string>/var/log/localdns-pf.out</string>
  </dict>
</plist>
